{
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "es2018"
    ],
    "module": "es2020",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": [
      "jest"
    ]
  },
  "exclude": [
    "**/*.test.ts"
  ]
}